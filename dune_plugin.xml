<dune_plugin>
    <name>Weburg_TV</name>
    <caption>Weburg TV</caption>
    <auto_resume>
        <enable>yes</enable>
    </auto_resume>
    <entry_points>
        <entry_point>
            <parent_media_url>root://tv</parent_media_url>
            <media_url>tv_group_list</media_url>
            <caption>Weburg TV</caption>
            <icon_url>plugin_file://icons/WeburgTV.png</icon_url>
            <actions>
                <key_enter>
                    <type>plugin_tv_play</type>
                </key_enter>
                <key_play>
                    <type>plugin_tv_play</type>
                </key_play>
            </actions>
            <ip_address_required>yes</ip_address_required>
            <show_by_default>yes</show_by_default>
        </entry_point>
    </entry_points>
    <auto_start>
        <run_action_before_auto_resume>yes</run_action_before_auto_resume>
        <entry_point_media_url>tv_group_list</entry_point_media_url>
        <force_on_start>black_screen</force_on_start>
        <force_on_finish>main_screen</force_on_finish>
        <skip_initial_dialogs>yes</skip_initial_dialogs>
        <action>
            <type>plugin_tv_play</type>
        </action>
    </auto_start>
    <type>php</type>
    <params>
        <program>webtv.php</program>
    </params>
    <version_index>191015</version_index>
    <version>2.1.0</version>
</dune_plugin>
